{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Attach",
            "type": "python",
            "request": "attach",
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}/python/src/",
                    "remoteRoot": "/work"
                }
            ],
            "port": 5678,
            "host": "127.0.0.1"
        },
        {
            "name": "Docker: Attach to Node",
            "type": "node",
            "request": "attach",
            "remoteRoot": "/work/src/",
            "port": 9229,
            "address": "localhost",
            "localRoot": "${workspaceFolder}/nodejs/src/",
            "protocol": "inspector",
            "restart": true
        }
    ]
}
